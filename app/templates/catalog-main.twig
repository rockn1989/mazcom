{% extends "layouts/default.twig" %}
{% set title="Корень каталога" %}
{% set bread = ["Главная", "Корень каталога"] %}
{% set titleArray =["Двигатель", "Колесная формула", "Коробка передач", "Кабина", "Тип платформы"] %}
{% set val =["168 л.с.", "170 л.с.", "286 л.с."] %}
{% block content %}

	<section class="page-content">

		<div class="uk-container">
			<div class="breadcrumbs-wrapper">
				{% include "includes/breadcrumbs.twig" %}
			</div>
		</div>

		<!-- begin catalog -->
		<div class="catalog">
			<div class="uk-container">
				<div class="uk-grid">
					<div class="uk-width-4-4@l">
						
						<!-- begin catalog-body -->
						<div class="catalog-body">
							<div class="uk-grid uk-grid-match">
							{% for tabInsert in 1..6 %}
								<div class="uk-width-1-3@s uk-width-1-4@m">
									<div class="card">
										<div class="card__img">
											<a href="">
												<img class="lazy" data-src="img/catalog-img-{{tabInsert}}.png" alt="">
											</a>
										</div>
										<div class="card__text">
											<div class="card__product-name">
												<a href="">МАЗ 5440A9-1320-031P1</a>
											</div>
										</div>
									</div>
								</div>
							{% endfor %}
							</div>
						</div><!-- end catalog-body -->

					</div>
				</div>
			</div>
		</div><!-- end catalog -->
	</section>
{% endblock %}
